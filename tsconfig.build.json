{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "declaration": true,
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "sourceMap": true
  },
  "include": ["nodes", "credentials", "index.ts"],
  "exclude": ["test", "dist", "node_modules"]
}
